<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="estate_property_type_tree_veiw" model="ir.ui.view">
            <field name="name">estate.property.tree</field>
            <field name="model">estate.property</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    
                </tree>
            </field>
        </record>
        <record id="estate_property_type_form_veiw" model="ir.ui.view">
            <field name="name">estate.property.form</field>
            <field name="model">estate.property</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                           
                        </group>
                        
                    </sheet>
                </form>
            </field>

        </record>
        <record id="action_property_type" model="ir.actions.act_window">
            <field name="name">Propertyyyyy</field>
            <field name="res_model">estate.property.type</field>
            <field name="view_mode">form,tree</field>
            <field name="view_id" ref="estate_property_type_tree_veiw"></field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                     Create a property
                </p>
            </field>
        </record>
    </data>
</odoo>
